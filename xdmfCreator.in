# ----- Results HDF files -----
# 2d files
filePrefixRes2d  "results2d_" # Filename prefix for 2d h5 datasets
fileExtRes2d     "h5"         # Filename extension
grpNameTimeRes2d "data/var2d/time" # HDF group path to time
grpNameStepRes2d "data/var2d/step" # HDF group path to step
attrPosRes2d     2 # index of field name in group, e.g., /data/var2d/pr/000100 has "pr" at attrPos=2 (0-based index)
# 3d files
filePrefixRes3d  "results3d_" # Filename prefix for 3d h5 datasets
fileExtRes3d     "h5"         # Filename extension
grpNameTimeRes3d "data/var3d/time" # HDF group path to time
grpNameStepRes3d "data/var3d/step" # HDF group path to step
attrPosRes3d     2 # index of field name in group, e.g., /data/var3d/pr/000100 has "pr" at attrPos=2 (0-based index)
#
grpNameCoordsResx "/coords/x" # HDF group path to x,y,z coordinates
grpNameCoordsResy "/coords/y"
grpNameCoordsResz "/coords/z"
#
